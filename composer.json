{
    "name": "fr3d/psr3-message-assertions",
    "description": "Test your log messages are compliant with the PSR-3 specification",
    "keywords": [
        "assertion",
        "log",
        "phpunit",
        "psr-3",
        "test",
        "testing"
    ],
    "license": "BSD-2-Clause",
    "homepage": "https://github.com/Maks3w/Psr3MessageAssertions",
    "require": {
        "php": ">= 7.1",
        "psr/log": "~1.0",
        "phpunit/phpunit": "^6.0||^7.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "FR3D\\Psr3MessagesAssertions\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FR3D\\Psr3MessagesAssertions\\": "tests/"
        }
    }
}
